    <item name="changePage" type="20">
      <varValue />
      <functionParam>pageName</functionParam>
      <functionCode>if (g5.currentPageName == pageName) return;
var page = null;
if (g5.currentPageName == "p1") {
    page = g5.pages.p1;
} else if (g5.currentPageName == "p2") {
    page = g5.pages.p2;
} else if (g5.currentPageName == "p3") {
    page = g5.pages.p3;
}
if (page) {
    if (page.deactive) {
        page.deactive();
    }
    ee.setVisible(page, false);
}

g5.currentPageName = pageName;
page = null;
if (g5.currentPageName == "p1") {
    page = g5.pages.p1;
} else if (g5.currentPageName == "p2") {
    page = g5.pages.p2;
} else if (g5.currentPageName == "p3") {
    page = g5.pages.p3;
}
if (page) {
    if (page.active) {
        page.active();
    }
    ee.setVisible(page, true);
}
</functionCode>
